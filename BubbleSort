public static int[] bubbleSort(int a[])
{
    for(int i=0;i<a.length-1;i++)
    {
        for(int j=0;j<a.length-1-i;j++)
        {
            if(a[j]>a[j+1])
            {
                int temp = a[j];
                a[j] = a[j+1];
                a[j+1] = temp;
            }
        }
    }
    return a;
}

public static void main (int a[]
{
    int a[] = {1,5,3,5,3,58,2,82,6};

    System.out.println(Arrays.toString(bubbleSort(a)));

}
